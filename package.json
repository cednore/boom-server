{
  "name": "boom-server",
  "version": "1.2.0",
  "description": "Proxy server between Laravel API and socket.io clients",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node cli.js start",
    "stop": "node cli.js stop",
    "restart": "node cli.js start --force",
    "prepare": "npm run build"
  },
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "git@github.com:cednore/boom-server.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "boom",
    "boom-server",
    "socket.io",
    "server"
  ],
  "author": "cednore <cednore@gmail.com>",
  "license": "MIT",
  "typings": "index.d.ts",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "dotenv": "^7.0.0",
    "express": "^5.0.0-alpha.1",
    "inquirer": "^6.2.2",
    "lodash": "^4.17.15",
    "memcached": "^2.2.2",
    "mysql2": "^1.6.5",
    "slug": "^1.0.0",
    "socket.io": "^2.2.0",
    "socketio-auth": "^0.1.1",
    "socketio-jwt": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^12.6.9",
    "cross-env": "^5.2.0",
    "tar": "^4.4.8",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  },
  "bin": {
    "boom-server": "bin/boom-server.js"
  }
}
